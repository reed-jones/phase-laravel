{
    "name": "reed-jones/phase",
    "description": "Transparent Vue <-> Laravel Integration",
    "type": "library",
    "license": "ISC",
    "authors": [
        {
            "name": "Reed Jones",
            "email": "reedjones@reedjones.com"
        }
    ],
    "require": {
        "vuexcellent/laravel": "^2.0",
        "reed-jones/phase-preset": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "ReedJones\\Phase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReedJones\\Phase\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ReedJones\\Phase\\PhaseServiceProvider"
            ],
            "aliases": {
                "Phase": "ReedJones\\Phase\\Facades\\Phase"
            }
        }
    }
}
